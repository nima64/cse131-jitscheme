(fun (ping n)
  (if (= (cast Num n) (cast Num 0))
    (cast Num n)
    (+ (cast Num (pong (cast Num (sub1 (cast Num n))))) (cast Num 1))
  )
)

(fun (pong n)
  (if (= (cast Num n) (cast Num 0))
    (cast Num n)
    (cast Num (ping (cast Num (sub1 (cast Num n)))))
  )
)

(print (ping (cast Num 50)))
